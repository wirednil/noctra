# DuckDB Environment Variables
# Source this file before building/testing: source duckdb.env

# DuckDB precompiled library location
export DUCKDB_LIB_DIR=/opt/duckdb
export DUCKDB_INCLUDE_DIR=/opt/duckdb
export LD_LIBRARY_PATH=/opt/duckdb:$LD_LIBRARY_PATH

